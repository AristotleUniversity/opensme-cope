body{
	background-color: #EEE;
	color:#000;
	font-family: Arial;
	font-size: 14px;
}

blockquote {
	text-align: left;
	border: 2px solid #000;
	background-color: #FFF;
	padding: 10px;
	width:90% !important;
	min-width:95% !important;
	height:auto;
	min-height:70px !important;
	color: #444;
}

.bgcolor1 {
	background-color: #CCF;
}

.bgcolor2 {
	background-color: #FFF;
}

.methodFrame{
	float: left;
	border: 2px solid #000;
}


.menu h3{
	color: #000;
	font-style: bold;
}

#menuTitle{
	margin-left: 10px;
}

#validation a {
	margin-left: 10px;
}

.menu #menu_contents a {
	margin-left: 10px;
}
.rowname {
	font-size: 14px;
}

.tablecon {
	font-size: 13px;
}

table p{
	color:#444;
}

.num {
	border: 1px solid #AAA;
	color: #000;
	width: 5%;
	height: auto;
	text-align: center;
}

.code {
	padding-left: 5px;
	background-color: #fff;
	border: 1px solid #CCC;
	color: #000;
	width: 100%;
	height: auto;
}

.covered {
	background-color: #00FF00;
	border: 1px solid #00FF00;
}

.uncovered {
	background-color: #FF0000;
	border: 1px solid #FF0000;
}

#jj_paths{
	color:#444; 
	float:left;
}

#source_code {
	color:#444; 
	float:left; 
	width:70%;
}

#xml {
	text-align: left;
	overflow: scroll;
	padding: 10px;
	width:95%;
	height:75%;
	background-color:#fff;
	font-size:15px;
	border:1px solid #000;
}

.container h2{
	color:#444;
}

.container h2 span{
	color:#000;
}
